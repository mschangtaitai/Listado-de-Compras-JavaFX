<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddList">
   <children>
      <Label layoutX="107.0" layoutY="82.0" text="Crear nueva lista">
         <font>
            <Font name="Ayuthaya" size="26.0" />
         </font>
      </Label>
      <Label layoutX="114.0" layoutY="152.0" text="Nombre:">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label layoutX="114.0" layoutY="200.0" text="Descripcion:">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="listName" layoutX="240.0" layoutY="150.0" prefHeight="27.0" prefWidth="281.0" />
      <TextField fx:id="listDescription" layoutX="240.0" layoutY="198.0" prefHeight="129.0" prefWidth="281.0" />
      <Button fx:id="cancelButton" layoutX="372.0" layoutY="346.0" mnemonicParsing="false" onAction="#onCancel" text="Cancelar" />
      <Button fx:id="addListButton" layoutX="462.0" layoutY="346.0" mnemonicParsing="false" onAction="#onClickAddList" text="Crear" />
   </children>
</AnchorPane>
